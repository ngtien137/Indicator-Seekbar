<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndicatorSeekbar">
        <attr name="is_thumb" format="reference"/>
        <attr name="is_indicator" format="reference"/>
        <attr name="is_thumb_size" format="dimension"/>
        <attr name="is_seekbar_corners" format="dimension"/>
        <attr name="is_seekbar_color" format="color"/>
        <attr name="is_seekbar_progress_color" format="color"/>
        <attr name="is_seekbar_height" format="dimension"/>
        <attr name="is_thumb_touch_extra_area" format="dimension"/>
        <attr name="is_max" format="integer"/>
        <attr name="is_min" format="integer"/>
        <attr name="is_progress" format="integer"/>

        <attr name="is_indicator_width" format="dimension"/>
        <attr name="is_indicator_height" format="dimension"/>
        <attr name="is_number_indicator" format="integer"/>
        <attr name="is_text_indicator_size" format="dimension"/>
        <attr name="is_text_indicator_color" format="color"/>
        <attr name="is_show_indicator_text" format="boolean"/>
        <attr name="is_space_text_indicator_to_bar" format="dimension"/>

        <attr name="is_show_progress_value" format="boolean"/>
        <attr name="is_text_value_color" format="color"/>
        <attr name="is_text_value_size" format="dimension"/>
        <attr name="is_space_text_value_to_bar" format="dimension"/>
    </declare-styleable>
</resources>